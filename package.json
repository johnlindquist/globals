{
  "name": "@johnlindquist/globals",
  "author": "John Lindquist (https://johnlindquist.com)",
  "license": "ISC",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public"
  },
  "description": "Shaving off npm install size of Kit",
  "type": "module",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "files": [
    "dist/*",
    "types/*"
  ],
  "scripts": {
    "test": "ava",
    "create-entry": "node ./build-entry.js",
    "build-esm": "node ./build-esm.js",
    "build-cjs": "node ./build-cjs.js",
    "build-types": "node ./build-types.js",
    "build": "npm run create-entry && npm run build-cjs && npm run build-esm && npm run build-types"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "24.1.0",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "11.1.0",
    "@types/axios": "0.14.0",
    "@types/chalk": "2.2.0",
    "@types/degit": "2.8.3",
    "@types/download": "8.0.2",
    "@types/fs-extra": "11.0.1",
    "@types/glob": "8.1.0",
    "@types/handlebars": "4.1.0",
    "@types/lodash": "4.14.194",
    "@types/marked": "4.3.0",
    "@types/minimist": "1.2.2",
    "@types/shelljs": "0.8.12",
    "@types/uuid": "9.0.1",
    "axios": "^1.4.0",
    "chalk": "5.0.1",
    "clipboardy": "3.0.0",
    "concurrently": "7.4.0",
    "degit": "2.8.4",
    "download": "^8.0.0",
    "dts-bundle-generator": "6.13.0",
    "esbuild": "0.17.18",
    "fs-extra": "11.1.1",
    "glob": "10.2.2",
    "handlebars": "4.7.7",
    "marked": "5.0.0",
    "minimist": "1.2.8",
    "replace-in-file": "6.3.5",
    "rollup": "3.21.4",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-ignore": "1.0.10",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "19.0.5",
    "semantic-release-plugin-update-version-in-files": "1.1.0",
    "shelljs": "0.8.5",
    "typescript": "5.0.4",
    "uuid": "9.0.0"
  },
  "release": {
    "branches": [
      "+([0-9]).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  },
  "dependencies": {
    "chalk-template": "^0.4.0",
    "execa": "7.1.1",
    "open": "9.1.0"
  }
}
